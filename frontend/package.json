{
  "name": "logistics-dashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.3.4",
    "chart.js": "^4.2.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.29.3",
    "framer-motion": "^12.23.6",
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.7.3",
    "lucide-react": "^0.127.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-i18next": "^13.2.2",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.1",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^2.1.4",
    "zod": "^3.21.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:analyze": "GENERATE_SOURCEMAP=true npm run build && npm run analyze",
    "analyze": "source-map-explorer 'build/static/js/*.js' --html bundle-analysis.html",
    "analyze:components": "node scripts/analyze-components.js src/components",
    "analyze:components:win": "scripts\\analyze.bat src\\components",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx --max-warnings=0",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "lint:css": "eslint src --ext .js,.jsx --rule 'tailwindcss/no-custom-classname: error' --rule 'tailwindcss/classnames-order: error'",
    "lint:naming": "node scripts/enforce-naming-convention.js",
    "rename:kebab": "node scripts/rename-to-kebab-case.js",
    "update:imports": "node scripts/update-imports.js",
    "fix:ui-imports": "node scripts/fix-ui-imports.js src",
    "naming:fix": "npm run rename:kebab && npm run update:imports",
    "find:hardcoded": "node scripts/replace-hardcoded-values.js",
    "format": "prettier --write \"src/**/*.{js,jsx,json,css,md}\"",
    "extract-translations": "i18next-scanner --config i18next-scanner.config.js 'src/**/*.{js,jsx}'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "cd .. && husky install frontend/.husky"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/preset-create-react-app": "^7.0.2",
    "@storybook/react": "^7.0.2",
    "@storybook/react-webpack5": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "autoprefixer": "^10.4.14",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chalk": "^4.1.2",
    "eslint": "^8.36.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^8.0.3",
    "i18next-scanner": "^4.4.0",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "source-map-explorer": "^2.5.3",
    "storybook": "^7.0.2",
    "tailwindcss": "^3.2.7",
    "webpack": "^5.76.3",
    "webpack-bundle-analyzer": "^4.9.1"
  }
}
